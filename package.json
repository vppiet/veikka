{
    "name": "veikka",
    "version": "0.1.0",
    "description": "Veikka The IRC Bot",
    "main": "dist/deploy.js",
    "scripts": {
        "start": "ts-node deploy.ts",
        "build": "npx tsc",
        "dev": "npx nodemon",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
        "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testMatch '**/__tests__/**/?(*.)+(spec).[tj]s?(x)'",
        "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testMatch '**/__tests__/**/?(*.)+(ispec).[tj]s?(x)'"
    },
    "type": "module",
    "keywords": [
        "IRC"
    ],
    "author": "Ville Pietarinen",
    "license": "MIT",
    "dependencies": {
        "@types/lodash-es": "^4.17.7",
        "axios": "^1.3.4",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "irc-framework": "^4.13.1",
        "lodash-es": "^4.17.21",
        "ts-node": "^10.9.1",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@definitelytyped/dtslint": "^0.0.151",
        "@jest/globals": "^29.5.0",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-google": "^0.14.0",
        "jest": "^29.4.3",
        "nodemon": "^2.0.20",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    }
}
